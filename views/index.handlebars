<div class="example">CA 135-506</div>
<form action="/reg_number" method="POST">
  <div>
    <label for="Header">Registration number</label>
    <input type="text" name="textNumItem" placeholder="enter registration number" class="regTextElem">
    <button class="addBtn" type="submit" name="addbutton">Add</button>
  </div>

  {{#if messages.info}}
  <div class="entry">
    <h1> {{messages.info}}</h1>
  </div>
  {{/if}}

  <div class="regDiv">
    <ul class="regPlates">{{#each display}}
      <li>{{this.reg}}</li>
      {{/each}}</ul>
  </div>

  <div class="error"></div>
</form>

<form action="/reg_number" method="GET">
<div class="regDiv2">
    <ul class="regPlates2">{{#each display2}}
      <li>{{this.reg}}</li>
      {{/each}}</ul>
  </div>
  <select id="dropDown" name="dropDown">
    <option value='All' selected="selected">Show All</option>
    <option value='1'>Cape Town</option>
    <option value='2'>Bellville</option>
    <option value='3'>Stellenbosch</option>
  </select>

  <button class="filterBtn" type="submit" name="filterButton">Filter</button>
</form>

<form action="/reset" method="POST">
  <div>
    <button class="resetBtn" type="submit" name="resetbutton">Reset</button>
  </div>
</form>

<script src="registration.js" charset="utf-8"></script>

</html>